{
  "name": "storage-server-2",
  "version": "1.0.0",
  "description": "Attempt a different approach",
  "main": "index.js",
  "scripts": {
    "test": "jest --testPathIgnorePatterns=commonMocks.js --coverage --verbose",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "clean": "rimraf dist",
    "start": "node src/index.js",
    "start:prod": "node src/index.js",
    "start:dev": "nodemon src/index.js",
    "migrate:make": "npx knex migrate:make --knexfile knexfile.cjs",
    "migrate:latest": "npx knex migrate:latest --knexfile knexfile.cjs",
    "migrate:rollback": "npx knex migrate:rollback --knexfile knexfile.cjs",
    "seed:make": "npx knex seed:make --knexfile knexfile.cjs",
    "seed:run": "npx knex seed:run --knexfile knexfile.cjs",
    "docker:start": "docker compose -f ./bdd_docker/docker-compose.yaml up -d",
    "docker:stop": "docker compose -f ./bdd_docker/docker-compose.yaml down",
    "docker:cleanup": "docker compose -f ./bdd_docker/docker-compose.yaml down --volumes --remove-orphans",
    "bdd:setup": "npm run docker:start && npm run migrate:latest && npm run seed:run",
    "bdd:server": "npm run start",
    "bdd:health-check": "node scripts/healthCheck.js",
    "bdd:test": "concurrently -k \"npm run bdd:server\" \"npm run bdd:run-tests\"",
    "bdd:run-tests": "npm run bdd:setup && npm run bdd:health-check && npx cypress run",
    "bdd:cleanup": "npm run docker:cleanup"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.24.7",
    "@badeball/cypress-cucumber-preprocessor": "^21.0.3",
    "@cucumber/cucumber": "^11.1.0",
    "@jest/globals": "^29.7.0",
    "@types/express": "^4.17.21",
    "@types/mysql": "^2.15.26",
    "@types/node": "^20.12.12",
    "babel-jest": "^29.7.0",
    "chai": "^5.1.2",
    "concurrently": "^9.1.0",
    "cypress": "^13.16.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "jest-mock": "^29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "rimraf": "^5.0.7"
  },
  "dependencies": {
    "@apollo/server": "^4.11.2",
    "@graphql-tools/load-files": "^7.0.0",
    "@graphql-tools/merge": "^9.0.4",
    "@graphql-tools/schema": "^10.0.4",
    "axios": "^1.7.7",
    "body-parser": "^1.20.3",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-winston": "^4.2.0",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.2",
    "graphql-tag": "^2.12.6",
    "js-yaml": "^4.1.0",
    "knex": "^3.1.0",
    "moment": "^2.30.1",
    "mysql2": "^3.11.4",
    "natural": "^7.0.6",
    "sqlite3": "^5.1.7",
    "stopword": "^3.0.1",
    "testcontainers": "^10.10.0",
    "uuid": "^9.0.1",
    "winston": "^3.13.0"
  },
  "type": "module"
}
